page {
  height: 100%;
  background: #efeff5;
}
/* 默认区域 */
.normal-area {
  text-align: center;
  padding: 100rpx;
  background: #fff;
}
.normal-area .paySuccess-queue-num {
  font-size: 16px;
  color: red;
}
.normal-area .paySuccess-tip {
  margin: 15px 0 40px;
}
.normal-area .paySuccess-detail {
  color: #818181;
}
/* 有集集乐的页面 */
.has-collect-benefit .pay-success-item {
  margin-bottom: 30rpx;
  height: 100rpx;
  background-color:#fff;
}
.has-collect-benefit .pay-success-item .icon {
  display: inline-block;
  margin-top: 22rpx;
  margin-left: 40rpx;
  margin-right: 20rpx;
  vertical-align: top;
}
.has-collect-benefit .pay-success-item .text {
  display: inline-block;
  margin-top: 37rpx;
  vertical-align: top;
  color: #55b737;
}
.has-collect-benefit .to-store-area {
  margin-bottom: 30rpx;
  border-top: 1px solid #efeff5;
  color:#818181;
}
.has-collect-benefit .info-item {
  position: relative;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  border-bottom: 1px solid #efeff5;
}
.has-collect-benefit .info-item .item-name {
  position: absolute;
  top: 0;
  left: 0;
  display: inline-block;
  margin-left: 40rpx;
  text-align: left;
}
.has-collect-benefit .info-item .item-content {
  position: absolute;
  top: 0;
  right: 0;
  display: inline-block;
  margin-right: 40rpx;
  text-align: right;
  color: #000;
}
.has-collect-benefit .info-item .item-content.queue-num {
  color: #c72b22;
}
/* 集集乐区域 */
.collect-benefit-area {
  background: #fff;
  color:#818181;
}
.collect-item {
  position: relative;
  width: 100%;
  border-bottom: 1rpx solid #ddd;
  overflow:hidden;
}
.collect-item .star-num {
  margin: 35rpx 40rpx 50rpx;
}
.collect-item .star-list {
  margin: 0 auto 50rpx;
  width: 455rpx;
  text-align: left;
  font-size: 0;
}
.collect-item .star-list .star-item {
  display: inline-block;
  width: 70rpx;
  height: 70rpx;
  margin-right: 35rpx;
  margin-bottom: 15rpx;
}
.collect-item .star-list .star-item:nth-of-type(4n-3) {
  margin-left: 15px;
}
.collect-item .star-full {
  position: absolute;
  top: -30rpx;
  right: -30rpx;
  transform: rotate(-25deg);
  padding: 8rpx;
  border: 1px solid #fd888a;
  border-radius: 50%;
}
.collect-item .star-full-text {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 55rpx;
  text-align: center;
  color: #fd888a;
  border: 1px solid #fd888a;
  border-radius: 50%;
}
.collect-benefit-area .tip-item {
  line-height: 60rpx;
  padding: 20rpx 40rpx;
}
/* 按钮区域 */
.btn-area .finish-btn {
  display: block;
  margin: 35rpx;
  width: 680rpx;
  height: 85rpx;
  line-height: 85rpx;
  background-color: #ffc504;
  text-align: center;
  color: #000;
  border-radius: 8rpx;
}
.btn-area .check-order-btn {
  display: block;
  margin: 35rpx;
  width: 678rpx;
  height: 83rpx;
  line-height: 85rpx;
  border: 1rpx solide #e5e6ea;
  background-color: #fff;
  border-radius: 8rpx;
  color: #000;
  text-align: center;
}